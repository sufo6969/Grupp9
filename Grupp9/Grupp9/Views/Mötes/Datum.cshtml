@model Grupp9.Models.DatumViewModel

@{
    ViewBag.Title = "Datum";
}

@using (Html.BeginForm())
{
    <br />
    @Html.EditorFor(model => model.FörslagDatum, new { htmlAttributes = new { @class = "datepicker" } })

}

<link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />

@section scripts{

    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <script>
        $(function () {
            $(".datepicker").datepicker();
        });
    </script>
}